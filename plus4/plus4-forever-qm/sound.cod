SET_ADLIBSND:   XOR     BX,BX
.L1:            MOV     AX,[ADLIB_VECS+BX]
                MOV     [RESET_SOUND+BX],AX
                ADD     BX,2
                CMP     BL,14
                JNZ     .L1

                RETN

SET_TIMERSND:   XOR     BX,BX
.L1:            MOV     AX,[SPEAKER_VECS+BX]
                MOV     [RESET_SOUND+BX],AX
                ADD     BX,2
                CMP     BL,14
                JNZ     .L1

                RETN

SET_NOSND:      XOR     BX,BX
.L0:            MOV     AX,NO_SOUND
.L1:            MOV     [RESET_SOUND+BX],AX
                ADD     BX,2
                CMP     BL,14
                JNZ     .L1

NO_SOUND:       RETN

SET_SBSND:      XOR     BX,BX
                CMP     [SB_ST],BL
                JE      SET_NOSND.L0

.L1:            MOV     AX,[SB_VECS+BX]
                MOV     [RESET_SOUND+BX],AX
                ADD     BX,2
                CMP     BL,14
                JNZ     .L1

                RETN

RELOAD_SND:     MOV     BX,0FF0EH
.L1:            CALL    MREAD_DH
                CALL    MSTORE_DH
                INC     BX
                CMP     BL,12H
                JBE     .L1

.L0:            RETN

SILENCE:        XOR     DH,DH
                JMP     [SOUND_11]

RESTSOUND:      MOV     BX,0FF11H
                MOV     DH,[TED_11]
                JMP     MSTORE_DH

EXIT_SND_ENTRY: CMP     [EXIT_SOUND],0
                JE      RELOAD_SND.L0

                JMP     [EXIT_SOUND]

