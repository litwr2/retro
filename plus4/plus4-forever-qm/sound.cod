SET_ADLIBSND:   XOR     BX,BX
.L1:            MOV     AX,[ADLIB_VECS+BX]
                MOV     [RESET_SOUND+BX],AX
                ADD     BX,2
                CMP     BL,14
                JNZ     .L1

                RETN

SET_TIMERSND:   XOR     BX,BX
.L1:            MOV     AX,[SPEAKER_VECS+BX]
                MOV     [RESET_SOUND+BX],AX
                ADD     BX,2
                CMP     BL,14
                JNZ     .L1

                RETN

SET_NOSND:      XOR     BX,BX
.L1:            MOV     AX,NO_SOUND
                MOV     [RESET_SOUND+BX],AX
                ADD     BX,2
                CMP     BL,14
                JNZ     .L1

NO_SOUND:       RETN

SET_SBSND:      JMP     SET_NOSND

RELOAD_SND:     MOV     BX,0FF0EH
.L1:            CALL    MREAD_DH
                CALL    MSTORE_DH
                INC     BX
                CMP     BL,12H
                JBE     .L1

                RETN

SILENCE:        XOR     DH,DH
                JMP     [SOUND_11]

RESTSOUND:      MOV     BX,0FF11H
                MOV     DH,[TED_11]
                JMP     MSTORE_DH

