                MOV     [IEC#NU#BUF0L+SI],0
                MOV     [IEC#NU#BUF0P+SI],0
                MOV     [IEC#NU#DIR],0FFFFH
                JMP     IEC#NU#ENTD64

.NOTDIR:        CMP     [IEC#NU#CFN],'#'
