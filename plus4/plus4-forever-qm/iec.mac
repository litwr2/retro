                RETN

.OPEN:          CMP     [IEC#NU#CFT],'A'
                JZ      IEC#NU#ERR75

.L8:            CMP     [IEC#NU#CFT],'R'
                JZ      IEC#NU#ERR75

                CMP     [IEC#NU#CFA],'R'
                JZ      .L4

                CMP     [IEC#NU#CFA],'W'
                JZ      .LS0

                CALL    IEC#NU#FM64
                OR      BX,BX
                JS      IEC#NU#ERR62

                MOV     AL,[CBM_TY_LIST+EBX*4]
                MOV     [IEC#NU#CFT],AL
                MOV     [IEC#NU#CFA],'R'
