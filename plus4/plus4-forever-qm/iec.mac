.LS6:           INC     BX
                CALL    IEC#NU#GETFS64
                OR      BX,BX
                JZ      IEC#NU#ERR72

                CMP     BL,18
                JZ      .LS6

                MOV     WORD [IOBUF+1+SI],BX
                SHR     DI,1                    ;USE SAVED SI
                MOV     [IEC#NU#FHST+DI],1
                SHL     DI,9
                MOV     WORD [DS:IEC#NU#BUF0-2+DI],BX

                MOV     BX,[DEV#NU#FH]          ;WRITE DE
                CALL    SEEK_D64_POS
                MOV     AH,40H
                MOV     CX,256
                MOV     DX,IOBUF
