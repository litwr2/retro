.LOAD:          CMP     [IEC#NU#CNI+SI],0          ;LOAD?
                JNZ     .SAVE

.L4:            CALL    IEC#NU#FM64
                OR      BX,BX
                JS      IEC#NU#ERR62

                MOVZX   SI,[IEC#NU#CH]
                MOV     [IEC#NU#AM+SI],'R'
                MOV     [IEC#NU#FHST+SI],1
                SHL     SI,1
                MOV     [IEC#NU#BUF0L+SI],0
                MOV     [IEC#NU#BUF0P+SI],0
                MOV     AX,[CBM_FP_LIST+2*EBX]
                SHL     SI,8
                MOV     WORD [IEC#NU#BUF0-2+SI],AX
                RETN

.SAVE:          CMP     [IEC#NU#CNI+SI],1
                JNZ     .OPEN

.LS0:           CALL    IEC#NU#FM64
                OR      BX,BX
                JS      .LS7

                CMP     [IEC#NU#CFRS],0
                JE      IEC#NU#ERR63

                JMP     IEC#NU#ERR75

.LS7:           CMP     [IEC#NU#CFT],'X'
                JNZ     .LS1

                MOV     [IEC#NU#CFT],'P'
.LS1:           CMP     [IEC#NU#CFMCS],0
                JNE     IEC#NU#ERR33

                MOVZX   SI,[IEC#NU#CH]
                MOV     [IEC#NU#AM+SI],'W'
                SHL     SI,1
                MOV     [IEC#NU#BUF0L+SI],0
                MOV     [IEC#NU#BUF0P+SI],0
                SHL     SI,8
                MOV     [IEC#NU#BUF0+256+SI],0  ;FILESIZE
                SHR     SI,8
                CALL    IEC#NU#GETFDE64
                OR      BL,BL
                JZ      IEC#NU#ERR72

                MOV     AX,[D64_POS]
                MOV     AL,BL
                MOV     [IEC#NU#CHFH+SI],AX     ;DE POINTER
                PUSH    BX
                MOV     BX,[DEV#NU#FH]          ;READ DE
                CALL    SEEK_D64_POS
                MOV     AH,3FH
                MOV     CX,256
                MOV     DX,IOBUF
                INT     21H

                MOV     AL,1
                CMP     [IEC#NU#CFT],'S'
