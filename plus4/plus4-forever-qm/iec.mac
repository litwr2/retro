                INC     AX
                CMP     [IEC#NU#CFT],'P'
                JZ      .LS2

                INC     AX
.LS2:           POP     BX
                MOV     [IOBUF+BX],AL           ;FILL FT
                MOV     DWORD [IOBUF+19+BX],0
                MOV     DWORD [IOBUF+23+BX],0
                MOV     [IOBUF+27+BX],0
                MOV     WORD [IOBUF+28+BX],0    ;FILESIZE
