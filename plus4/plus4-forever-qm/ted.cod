                JS      V1_00

                MOV     SI,WORD [TED_1A]
                CMP     SI,WORD [TED_C]
                JNZ     T01000

                MOVZX   SI,DL
                MOV     CH,[GS:EBX+ESI*8]       ;MATRIX LINE
                NOT     CH
                JMP     T00000.L0A

V1_00:          MOV     AX,[COLOR0]
                MOV     [DI],AX
                MOV     [DI+2],AX
                MOV     [DI+4],AX
                MOV     [DI+6],AX
                ADD     DI,8
