T01000:         MOVZX   SI,DL           ;*** HRV+
                MOV     CH,[GS:EBX+ESI*8]       ;MATRIX LINE
                JMP     T00000.L0A

T00000:         MOV     SI,DX           ;***
                AND     SI,7FH
                MOV     CH,[GS:EBX+ESI*8]       ;MATRIX LINE
                OR      DL,DL
                JNS     .L0A

.L0B:           NOT     CH
.L0A:           MOV     CL,[TED_15]
.L0:            TEST    CH,16
                MOV     AL,CL
                JZ      .L1

                MOV     AL,DH
